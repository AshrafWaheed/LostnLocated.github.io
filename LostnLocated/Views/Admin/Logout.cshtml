@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <script src="~/Content/js/code.jquery.com_jquery-3.7.0.min.js"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm" crossorigin="anonymous"></script>
    <title>Logout</title>
    <script>$(document).ready(function () {
            window.history.forward();
            window.setTimeout("window.location.href='/General/Index/'", 3000);
        });</script>
</head>
<body>
    <div>
        <h2>You are Logged Out Successfully.</h2>
        @Html.ActionLink("CLICK HERE", "Login", "General", new { @class = "btn btn-lg btn-success" }) To Go Home.
    </div>
</body>
</html>